{
  "name": "coffee-api",
  "version": "1.0.0",
  "description": "coffee recipes api",
  "main": "index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "node --no-warnings --enable-source-maps --loader ts-node/esm src/index.ts",
    "debug": "node --inspect-brk --no-warnings --enable-source-maps --loader ts-node/esm src/index.ts"
  },
  "keywords": [
    "coffee",
    "recipes"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "author": "Alex Martinez",
  "license": "ISC"
}
